package Day2;

public class Strpgm {
	public static void main(String[] args) {
		String s="Welcome java1",s2="";
		int small=0,caps=0,num=0;
		for(int i=0;i<s.length();i++) {
			if('a'>=s.charAt(i) && s.charAt(i)<='z') {
				small++;
				s2=s2+s.charAt(i);
			}
		}System.out.println(small+s2);
	}

}
